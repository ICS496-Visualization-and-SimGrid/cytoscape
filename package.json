{
  "name": "sbgn-renderer",
  "version": "1.0.0",
  "description": "A library that renders SBGN",
  "main": "dist/sbgn-renderer.js",
  "scripts": {
    "test": "mocha --compilers js:babel-register --require babel-polyfill",
    "clean": "rm -rf demo/browserify-bundle.js dist/*",
    "build-demo": "browserify -e demo/browserify-entry.js -o demo/browserify-bundle.js -d -t [ babelify --comments false ]",
    "watch-demo": "watchify -e demo/browserify-entry.js -o demo/browserify-bundle.js -d -t [ babelify --comments false ] -p [ browserify-livereload --host 127.0.0.1 --port 30303 ] --poll=50 --verbose",
    "build": "browserify --standalone sbgn-renderer -e src/index.js -d -t [ babelify --comments false ] | derequire > dist/sbgn-renderer.js",
    "watch": "watchify --standalone sbgn-renderer -e src/index.js -d -t [ babelify --comments false ] -o 'derequire > dist/sbgn-renderer.js' --poll=50 --verbose",
    "build-test": "browserify test/*.js -o test/testenv/test-bundle.js -d",
    "watch-test": "watchify test/*.js -o test/testenv/test-bundle.js -d --poll=50 --verbose"
  },
  "keywords": [
    "biology",
    "canvas",
    "render",
    "sbgn",
    "network",
    "graph",
    "visualization",
    "cytoscape",
    "cytoscape.js"
  ],
  "author": "Dylan Fong",
  "license": "MIT",
  "devDependencies": {
    "babel-polyfill": "^6.22.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.22.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-livereload": "^1.0.10",
    "chai": "^3.5.0",
    "cytoscape-expand-collapse": "^3.0.2",
    "derequire": "^2.0.6",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-react": "^6.7.1",
    "file-saver": "^1.3.3",
    "jquery": "^3.2.1",
    "mocha": "^3.2.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "cytoscape": "https://github.com/d2fong/cytoscape.js.git#barrel",
    "extend": "^3.0.0",
    "lodash.defaultsdeep": "^4.6.0",
    "lodash.memoize": "^4.1.2",
    "sbgnml-to-cytoscape": "^3.0.2",
    "text-width": "^1.2.0"
  }
}
